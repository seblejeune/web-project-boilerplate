{
  "name": "web-project-boilerplate",
  "version": "0.0.1-alpha",
  "description": "Boilerplate to start web project with gulp and sass",
  "main": "index.js",
  "scripts": {
    "clean-install": "rm -rf node_modules && npm install",
    "clean-build": "gulp --gulpfile tools/gulp/gulpfile.js clean:build",
    "clean-prod": "gulp --gulpfile tools/gulp/gulpfile.js clean:prod",
    "serve": "gulp --gulpfile tools/gulp/gulpfile.js serve",
    "build:dev": "gulp --gulpfile tools/gulp/gulpfile.js build",
    "build:test": "gulp --gulpfile tools/gulp/gulpfile.js test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actito/web-project-boilerplate.git"
  },
  "keywords": [
    "Web",
    "SASS",
    "Boilerplate"
  ],
